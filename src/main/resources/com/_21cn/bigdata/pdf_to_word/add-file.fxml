<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="rootLayout" alignment="CENTER" prefHeight="642.0" prefWidth="1066.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/8" fx:controller="com._21cn.bigdata.pdf_to_word.controller.ApplicationController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <ImageView fitHeight="354.0" fitWidth="316.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#onAddFileClick">
        <Image url="@../../../../static/add_file.png"/>
    </ImageView>
    <Label contentDisplay="CENTER" text="点击添加文件" fx:id="welcomeText">
        <font>
          <Font name="Courier New" size="18.0"/>
        </font>
    </Label>
</VBox>
